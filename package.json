{
	"name": "auto-time-tracker",
	"displayName": "auto-time-tracker",
	"description": "Automatically track your time on vs code",
	"repository": "https://github.com/tommaso1/vs-code-auto-time-tracking",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "auto-time-tracker.openReport",
				"title": "Open Report"
			},
			{
				"command": "auto-time-tracker.openReportRaw",
				"title": "Open Report Raw"
			},
			{
				"command": "auto-time-tracker.openReportDetailed",
				"title": "Open Report Detailed"
			},
			{
				"command": "auto-time-tracker.pauseTracking",
				"title": "Pause Tracking"
			},
			{
				"command": "auto-time-tracker.resumeTracking",
				"title": "Resume Tracking"
			},
			{
				"command": "auto-time-tracker.clearStorage",
				"title": "Clear tracker storage"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"pack": "vsce package",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/lodash": "^4.14.182",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}